{
	"compilerOptions": {
		"target": "es2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"noEmit": true,
		"verbatimModuleSyntax": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"skipLibCheck": true
	}
}
